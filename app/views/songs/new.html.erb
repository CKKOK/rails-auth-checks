<div class="container">
  <h1>Add Song</h1>
  <%= form_for @new_song do |f| %>
    <%= f.label("Name:")%>
    <%= f.text_field :name %>
    <%= f.label("Genre:")%>
    <%= f.collection_check_boxes :genre_ids, @genres, :id, :name %>
    <%= f.submit("Add", class: "btn btn-primary") %>
  <% end %>
</div>

<div class="container">
  <h1>Songs</h1>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Name</th>
      </tr>
    </thead>
    <tbody>
      <% @songs.order(:name).each do |song| %>
        <tr>
          <td>
            <%= link_to edit_song_path(song) do%><%= song.name %>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
